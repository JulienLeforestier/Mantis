<form method="post" action="{{ path('command_delete', {'id': command.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cette commande?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ command.id) }}">
	<button class="btn btn-primary">Supprimer</button>
</form>
