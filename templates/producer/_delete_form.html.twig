<form method="post" action="{{ path('producer_delete', {'id': producer.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer ce producteur?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ producer.id) }}">
	<button class="btn btn-primary">Supprimer</button>
</form>
